<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_center"
        android:layout_width="136dp"
        android:layout_height="124dp"
        android:layout_marginTop="@dimen/xxh_dp_24"
        android:src="@mipmap/vip_center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_vip_nick"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xxh_dp_20"
        android:layout_marginTop="@dimen/xxh_dp_16"
        android:src="@mipmap/vip_sbg_one"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_center" />

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ff950b7b"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@id/iv_vip_nick"
        app:layout_constraintEnd_toEndOf="@id/iv_vip_nick"
        app:layout_constraintStart_toStartOf="@id/iv_vip_nick"
        app:layout_constraintTop_toTopOf="@id/iv_vip_nick" />


</androidx.constraintlayout.widget.ConstraintLayout>