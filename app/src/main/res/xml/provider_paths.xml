<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!--本地安装包下载位置-->
    <cache-path
        name="cache"
        path="/temp"/>
    <external-cache-path
        name="exCache"
        path="/temp"/><!--name必须不一样  否则只生效最后一个-->
    <root-path
        name="sdcardCache"
        path="/mnt/sdcard/com.swartz.cicada/temp"/>
    <root-path
        name="scanner"
        path=""/>
    <external-path
        name="beta_external_path"
        path="temp/"/>

    <external-files-path
        name="beta_external_files_path"
        path="temp/"/>

    <!-- bugly  /storage/emulated/0/Download/${applicationId}/.beta/apk-->
    <external-path name="beta_external_path" path="Download/"/>
    <!--bugly   /storage/emulated/0/Android/data/${applicationId}/files/apk/-->
    <external-path name="beta_external_files_path" path="Android/data/"/>
</paths>

